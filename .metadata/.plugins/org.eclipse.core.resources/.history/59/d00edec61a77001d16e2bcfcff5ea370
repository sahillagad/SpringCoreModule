package com.masai;

public class Employee {

	private int eid;
	private String name;
	private double salary;
	
	
	public Employee(int eid, String name, double salary) {
		super();
		this.eid = eid;
		this.name = name;
		this.salary = salary;
	}


	public int getEid() {
		return eid;
	}


	public String getName() {
		return name;
	}


	public double getSalary() {
		return salary;
	}
 

	
	
	
}
